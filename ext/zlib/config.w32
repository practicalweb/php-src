ARG_ENABLE("zlib", "ZLIB support", "yes");

if (PHP_ZLIB == "yes") {
	EXTENSION("zlib", "zlib.c zlib_fopen_wrapper.c", null, "/D ZLIB_EXPORTS");
	AC_DEFINE("HAVE_ZLIB", 1, "Tokenizer support");
	ADD_FLAG("LIBS_ZLIB", "zlib.lib");
}


