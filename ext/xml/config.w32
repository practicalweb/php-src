ARG_WITH("xml", "XML support", "yes");

if (PHP_XML == "yes" && PHP_LIBXML == "yes") {
	EXTENSION("xml", "xml.c compat.c");
	AC_DEFINE("HAVE_XML", 1, "XML support");
	if (!PHP_XML_SHARED) {
		ADD_FLAG("CFLAGS_XML", "/D LIBXML_STATIC ");
	}
}


